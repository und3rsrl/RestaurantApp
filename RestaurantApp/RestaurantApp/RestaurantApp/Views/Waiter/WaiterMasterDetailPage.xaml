<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
                  xmlns:helpers="clr-namespace:RestaurantApp.Views.Waiter.Views"
                  x:Class="RestaurantApp.Views.Waiter.WaiterMasterDetailPage">
    <MasterDetailPage.Master>
        <helpers:WaiterMasterPage x:Name="masterView"/>
    </MasterDetailPage.Master>
    <MasterDetailPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <helpers:ActiveOrders/>
            </x:Arguments>
        </NavigationPage>
    </MasterDetailPage.Detail>
</MasterDetailPage>